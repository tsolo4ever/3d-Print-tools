  "stepCalculations": {
    "_comment": "Steps per mm calculations",
    
    "formulas": {
      "belt": "(steps_per_rev * microstepping) / (pulley_teeth * belt_pitch)",
      "leadscrew": "(steps_per_rev * microstepping) / lead",
      "directExtruder": "(steps_per_rev * microstepping) / (gear_circumference)",
      "gearedExtruder": "(steps_per_rev * microstepping * gear_ratio) / (drive_gear_circumference)"
    },
    
    "commonPulleys": {
