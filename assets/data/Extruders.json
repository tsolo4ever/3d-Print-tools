  "extruderTypes": {
    "Bowden": {
      "description": "Extruder mounted on frame, PTFE tube to hotend",
      "pros": ["Lighter toolhead", "Faster movement", "Simpler toolhead"],
      "cons": ["Worse retraction", "Bad for flexibles", "Retraction distance 4-6mm"],
      "typicalESteps": "85-100 (1:1 ratio)",
      "retractionRange": [4, 7]
    },
    "Direct Drive": {
      "description": "Extruder mounted directly above hotend",
