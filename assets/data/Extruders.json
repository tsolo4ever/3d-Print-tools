  "eStepsCalculation": {
    "formula": "E-Steps = (Motor Steps × Microstepping × Gear Ratio) / (π × Drive Gear Diameter)",
    "example": {
      "motorSteps": 200,
      "microstepping": 16,
      "gearRatio": 3,
      "driveGearDiameter": 8,
      "result": "200 × 16 × 3 / (3.14159 × 8) = 382 steps/mm"
    },
    "calibrationNote": "Always calibrate! Measure 100mm, extrude 100mm, measure actual, calculate: New E-Steps = Current E-Steps × (100 / Actual)"
  }
