    },
    
    "yMinPos": {
      "mapsFrom": ["Y_MIN_POS"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [0, -5]
    },
    
    "zMinPos": {
      "mapsFrom": ["Z_MIN_POS"],
      "type": "integer",
