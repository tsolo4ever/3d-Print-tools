{
  "$schema": "Marlin Configuration Field Mapping",
  "version": "1.0.0",
  "description": "Vanilla Marlin Configuration.h field mappings",
  "firmware": "Marlin",
  "configFile": "Configuration.h",
  
  "basic": {
    "configVersion": {
      "mapsFrom": ["CONFIGURATION_H_VERSION"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [2010300],
      "notes": "Marlin configuration version identifier"
    },
    
    "machineName": {
      "mapsFrom": ["CUSTOM_MACHINE_NAME"],
      "type": "string",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": ["3D Printer", "My Printer"],
      "validation": {
        "maxLength": 50
      }
    },
    
    "machineUUID": {
      "mapsFrom": ["MACHINE_UUID"],
      "type": "string",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": ["00000000-0000-0000-0000-000000000000"]
    },
    
    "motherboard": {
      "mapsFrom": ["MOTHERBOARD"],
      "type": "string",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": ["BOARD_RAMPS_14_EFB", "BOARD_BTT_SKR_V1_4"],
      "notes": "Board type from boards.h"
    },
    
    "serialPort": {
      "mapsFrom": ["SERIAL_PORT"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [0, -1],
      "validation": {
        "min": -1,
        "max": 9
      }
    },
    
    "baudRate": {
      "mapsFrom": ["BAUDRATE"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [250000, 115200],
      "validation": {
        "enum": [2400, 9600, 19200, 38400, 57600, 115200, 250000, 500000, 1000000]
      }
    },
    
    "serialPort2": {
      "mapsFrom": ["SERIAL_PORT_2"],
      "type": "integer",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": [-1, 1]
    },
    
    "baudRate2": {
      "mapsFrom": ["BAUDRATE_2"],
      "type": "integer",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": [250000]
    },
    
    "extruders": {
      "mapsFrom": ["EXTRUDERS"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [1, 2],
      "validation": {
        "min": 0,
        "max": 8
      }
    },
    
    "defaultFilamentDia": {
      "mapsFrom": ["DEFAULT_NOMINAL_FILAMENT_DIA"],
      "type": "float",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [1.75, 2.85, 3.0],
      "notes": "Expected filament diameter in mm"
    }
  },
  
  "hardware": {
    "driverX": {
      "mapsFrom": ["X_DRIVER_TYPE"],
      "type": "string",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": ["A4988", "TMC2209", "TMC2130"]
    },
    
    "driverY": {
      "mapsFrom": ["Y_DRIVER_TYPE"],
      "type": "string",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": ["A4988", "TMC2209"]
    },
    
    "driverZ": {
      "mapsFrom": ["Z_DRIVER_TYPE"],
      "type": "string",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": ["A4988", "TMC2209"]
    },
    
    "driverE0": {
      "mapsFrom": ["E0_DRIVER_TYPE"],
      "type": "string",
      "required": true,
      "fileLocation": "Configuration.h",
