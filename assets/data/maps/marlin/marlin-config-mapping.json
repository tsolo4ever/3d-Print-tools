    "jerkX": {
      "mapsFrom": ["DEFAULT_XJERK"],
      "type": "float",
      "required": false,
      "conditionalOn": ["CLASSIC_JERK"],
      "fileLocation": "Configuration.h",
      "examples": [10.0, 8.0]
    },
    
    "jerkY": {
      "mapsFrom": ["DEFAULT_YJERK"],
      "type": "float",
      "required": false,
      "conditionalOn": ["CLASSIC_JERK"],
      "fileLocation": "Configuration.h",
