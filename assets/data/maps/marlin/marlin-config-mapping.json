  "motion": {
    "stepsPerMM": {
      "mapsFrom": ["DEFAULT_AXIS_STEPS_PER_UNIT"],
      "type": "array",
      "arrayFields": ["x", "y", "z", "e"],
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [[80, 80, 400, 500], [80, 80, 400, 93]],
      "notes": "Steps per mm for X, Y, Z, E axes"
    },
    
    "maxFeedrate": {
      "mapsFrom": ["DEFAULT_MAX_FEEDRATE"],
      "type": "array",
      "arrayFields": ["x", "y", "z", "e"],
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [[300, 300, 5, 25]],
      "notes": "Maximum feedrate in mm/s"
    },
    
    "maxAcceleration": {
      "mapsFrom": ["DEFAULT_MAX_ACCELERATION"],
      "type": "array",
      "arrayFields": ["x", "y", "z", "e"],
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [[3000, 3000, 100, 10000]],
      "notes": "Maximum acceleration in mm/sÂ²"
    },
    
    "acceleration": {
      "mapsFrom": ["DEFAULT_ACCELERATION"],
      "type": "float",
      "required": true,
