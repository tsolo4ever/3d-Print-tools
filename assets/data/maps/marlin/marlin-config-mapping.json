{
  "$schema": "Marlin Configuration Field Mapping",
  "version": "1.0.0",
  "description": "Vanilla Marlin Configuration.h field mappings",
  "firmware": "Marlin",
  "configFile": "Configuration.h",
  
  "basic": {
    "configVersion": {
      "mapsFrom": ["CONFIGURATION_H_VERSION"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [2010300],
      "notes": "Marlin configuration version identifier"
    },
    
    "machineName": {
      "mapsFrom": ["CUSTOM_MACHINE_NAME"],
      "type": "string",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": ["3D Printer", "My Printer"],
      "validation": {
        "maxLength": 50
      },
      "uiId": "profileName",
      "uiTab": 1,
      "uiSection": "Basic Information",
      "uiOrder": 1,
      "uiWidget": "text",
      "helpText": "Friendly name for this printer profile"
    },
    
    "machineUUID": {
      "mapsFrom": ["MACHINE_UUID"],
      "type": "string",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": ["00000000-0000-0000-0000-000000000000"]
    },
    
    "motherboard": {
      "mapsFrom": ["MOTHERBOARD"],
      "type": "string",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": ["BOARD_RAMPS_14_EFB", "BOARD_BTT_SKR_V1_4"],
      "notes": "Board type from boards.h",
      "uiId": "hardwareBoard",
      "uiTab": 1,
      "uiSection": "Hardware",
      "uiOrder": 10,
      "uiWidget": "database-select",
      "uiDatabase": "marlin-boards-V2",
      "uiDisplayFormat": "{name} ({mcu})",
      "helpText": "Select your motherboard or choose Custom"
    },
    
    "serialPort": {
      "mapsFrom": ["SERIAL_PORT"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [0, -1],
      "validation": {
        "min": -1,
        "max": 9
      }
    },
    
    "baudRate": {
      "mapsFrom": ["BAUDRATE"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [250000, 115200],
      "validation": {
        "enum": [2400, 9600, 19200, 38400, 57600, 115200, 250000, 500000, 1000000]
      },
      "uiId": "baudRate",
      "uiTab": 1,
      "uiSection": "Communication",
      "uiOrder": 20,
      "uiWidget": "select",
      "helpText": "Serial communication speed (115200 or 250000 typical)"
    },
    
    "serialPort2": {
      "mapsFrom": ["SERIAL_PORT_2"],
      "type": "integer",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": [-1, 1]
