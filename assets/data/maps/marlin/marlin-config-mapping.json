      "required": false,
      "fileLocation": "Configuration.h",
      "examples": [250000]
    },
    
    "extruders": {
      "mapsFrom": ["EXTRUDERS"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [1, 2],
      "validation": {
        "min": 0,
        "max": 8
      },
      "uiId": "extruderCount",
      "uiTab": 1,
      "uiSection": "Hardware",
      "uiOrder": 11,
      "uiWidget": "number",
      "helpText": "Number of extruders (1-8, typically 1 or 2)"
