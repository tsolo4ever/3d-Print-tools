    "hotendMaxTemp": {
      "mapsFrom": ["HEATER_0_MAXTEMP"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [275, 300],
      "validation": {
        "min": 200,
        "max": 500
      }
    },
    
    "bedMinTemp": {
      "mapsFrom": ["BED_MINTEMP"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [5]
    },
    
    "bedMaxTemp": {
      "mapsFrom": ["BED_MAXTEMP"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [150, 130]
    },
    
    "pidTemp": {
      "mapsFrom": ["PIDTEMP"],
      "type": "boolean",
      "required": false,
      "fileLocation": "Configuration.h",
      "notes": "Enable PID for hotend"
    },
    
    "pidKp": {
      "mapsFrom": ["DEFAULT_KP"],
      "type": "float",
      "required": false,
      "conditionalOn": ["PIDTEMP"],
      "fileLocation": "Configuration.h",
      "examples": [22.20]
    },
    
    "pidKi": {
      "mapsFrom": ["DEFAULT_KI"],
      "type": "float",
      "required": false,
      "conditionalOn": ["PIDTEMP"],
      "fileLocation": "Configuration.h",
      "examples": [1.08]
    },
    
    "pidKd": {
      "mapsFrom": ["DEFAULT_KD"],
      "type": "float",
      "required": false,
      "conditionalOn": ["PIDTEMP"],
      "fileLocation": "Configuration.h",
      "examples": [114.00]
    },
    
    "pidTempBed": {
      "mapsFrom": ["PIDTEMPBED"],
      "type": "boolean",
      "required": false,
      "fileLocation": "Configuration.h",
      "notes": "Enable PID for bed"
    },
    
    "pidBedKp": {
      "mapsFrom": ["DEFAULT_BED_KP"],
      "type": "float",
      "required": false,
      "conditionalOn": ["PIDTEMPBED"],
      "fileLocation": "Configuration.h",
      "examples": [10.00]
    },
    
    "pidBedKi": {
      "mapsFrom": ["DEFAULT_BED_KI"],
      "type": "float",
      "required": false,
      "conditionalOn": ["PIDTEMPBED"],
      "fileLocation": "Configuration.h",
      "examples": [0.023]
    },
    
    "pidBedKd": {
      "mapsFrom": ["DEFAULT_BED_KD"],
      "type": "float",
      "required": false,
      "conditionalOn": ["PIDTEMPBED"],
      "fileLocation": "Configuration.h",
      "examples": [305.4]
    },
    
    "extrudeMinTemp": {
      "mapsFrom": ["EXTRUDE_MINTEMP"],
      "type": "integer",
      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [170],
      "notes": "Minimum temp for extrusion"
    },
    
    "preheat1Label": {
      "mapsFrom": ["PREHEAT_1_LABEL"],
      "type": "string",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": ["PLA"]
    },
    
    "preheat1TempHotend": {
      "mapsFrom": ["PREHEAT_1_TEMP_HOTEND"],
      "type": "integer",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": [180, 200]
    },
    
    "preheat1TempBed": {
      "mapsFrom": ["PREHEAT_1_TEMP_BED"],
      "type": "integer",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": [70, 60]
    },
    
    "preheat2Label": {
      "mapsFrom": ["PREHEAT_2_LABEL"],
      "type": "string",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": ["ABS"]
    },
    
    "preheat2TempHotend": {
      "mapsFrom": ["PREHEAT_2_TEMP_HOTEND"],
      "type": "integer",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": [240, 230]
    },
    
    "preheat2TempBed": {
      "mapsFrom": ["PREHEAT_2_TEMP_BED"],
      "type": "integer",
      "required": false,
      "fileLocation": "Configuration.h",
      "examples": [110, 100]
    }
  },
  
  "geometry": {
    "bedSizeX": {
      "mapsFrom": ["X_BED_SIZE"],
      "type": "integer",
      "required": true,
