      "required": true,
      "fileLocation": "Configuration.h",
      "examples": [3000, 1000]
    },
    
    "classicJerk": {
      "mapsFrom": ["CLASSIC_JERK"],
      "type": "boolean",
      "required": false,
      "fileLocation": "Configuration.h",
      "notes": "Enable classic jerk instead of junction deviation"
